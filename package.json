{
  "name": "mapnik",
  "description": "Tile rendering library for node",
  "url": "http://github.com/mapnik/node-mapnik",
  "homepage": "http://mapnik.org",
  "author": "Dane Springmeyer <dane@mapbox.com> (mapnik.org)",
  "version": "4.5.8",
  "mapnik_version": "e553f55dc",
  "main": "./lib/mapnik.js",
  "binary": {
    "module_name": "mapnik",
    "module_path": "./lib/binding/napi-v{napi_build_version}",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{platform}-{arch}-napi-v{napi_build_version}.tar.gz",
    "host": "https://mapbox-node-binary.s3.amazonaws.com",
    "napi_versions": [
      3
    ]
  },
  "bugs": {
    "email": "dane@mapbox.com",
    "url": "http://github.com/mapnik/node-mapnik/issues"
  },
  "keywords": [
    "map",
    "graphics",
    "canvas",
    "tile",
    "mapnik",
    "carto"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mapnik/node-mapnik.git"
  },
  "contributors": [
    "Konstantin KÃ¤fer",
    "Blake Thompson"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.5",
    "bindings": "^1.5.0",
    "cmake-js": "^6.2.1",
    "mapnik-vector-tile": "^3.0.1",
    "node-addon-api": "^3.2.1"
  },
  "bin": {
    "mapnik-index.js": "bin/mapnik-index.js",
    "mapnik-inspect.js": "bin/mapnik-inspect.js",
    "mapnik-render.js": "bin/mapnik-render.js",
    "mapnik-shapeindex.js": "bin/mapnik-shapeindex.js"
  },
  "scripts": {
    "test": "tape test/*.test.js",
    "clean": "node scripts/clean.js",
    "configure": "cmake-js configure --CDnapi_build_version=3 --target node-mapnik",
    "build": "npm run clean && npm run configure && cmake-js compile --CDnapi_build_version=3 --target node-mapnik",
    "install": "node-pre-gyp install || npm run build",
    "docs": "documentation build src/*.cpp src/mapnik_plugins.hpp --polyglot -o documentation -f html --github --name Mapnik",
    "package": "node-pre-gyp package",
    "publish": "npm run package && node-pre-gyp publish"
  },
  "devDependencies": {
    "@mapbox/sphericalmercator": "~1.0.2",
    "aws-sdk": "2.0.12",
    "bytes": "~2.1.0",
    "nyc": "^14.1.1",
    "tape": "^4.13.3"
  },
  "jshintConfig": {
    "node": true,
    "globalstrict": true,
    "undef": true,
    "unused": true,
    "noarg": true,
    "mocha": true
  }
}
